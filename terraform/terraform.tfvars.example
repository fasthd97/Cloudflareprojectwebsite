# Example Terraform variables file
# Copy this to terraform.tfvars and fill in your actual values

# Your Cloudflare account ID (found in dashboard sidebar)
cloudflare_account_id = "your-account-id-here"

# Your domain name (must be added to Cloudflare)
domain_name = "yourname.com"

# Your GitHub username
github_username = "yourusername"

# GitHub repository name
github_repo_name = "resume-website"

# Strong random string for JWT signing (generate with: openssl rand -base64 32)
jwt_secret = "your-super-secret-jwt-key-here"

# Admin email for initial setup
admin_email = "your.email@example.com"